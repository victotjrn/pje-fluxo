                <variable name="ef_fbg_publicar_dje" mapped-name="sim_nao:ef_fbg_publicar_dje" access="read,write"/>
                <variable name="ef_fbg_renajud_consultar" mapped-name="sim_nao:ef_fbg_renajud_consultar" access="read,write"/>
            </controller>
